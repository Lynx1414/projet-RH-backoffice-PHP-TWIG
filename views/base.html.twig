<!doctype html>
<html lang="en" class="h-100" data-bs-theme="auto">
	<head>
		<script src="/docs/5.3/assets/js/color-modes.js"></script>

		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta name="description" content="">
		<meta name="author" content="Mark Otto, Jacob Thornton, and Bootstrap contributors">
		<meta name="generator" content="Hugo 0.111.3">
		<title>
			{% block title %}
				{{titre}}
			{% endblock %}
			-
			{% block sousTitle %}{% endblock %}
		</title>

		<link rel="canonical" href="https://getbootstrap.com/docs/5.3/examples/sticky-footer-navbar/">


		<link rel="stylesheet" href="style.css">


		<link
		href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">

		<!-- Favicons -->
		<link rel="apple-touch-icon" href="/docs/5.3/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
		<link rel="icon" href="/docs/5.3/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
		<link rel="icon" href="/docs/5.3/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
		<link rel="manifest" href="/docs/5.3/assets/img/favicons/manifest.json">
		<link rel="mask-icon" href="/docs/5.3/assets/img/favicons/safari-pinned-tab.svg" color="#712cf9">
		<link rel="icon" href="/docs/5.3/assets/img/favicons/favicon.ico">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer"/>
		<meta name="theme-color" content="#712cf9">

		<style>
			body {
				padding: 60px;
			}
			.bd-placeholder-img {
				font-size: 1.125rem;
				text-anchor: middle;
				-webkit-user-select: none;
				-moz-user-select: none;
				user-select: none;
			}

			@media(min-width: 768px) {
				.bd-placeholder-img-lg {
					font-size: 3.5rem;
				}
			}

			.b-example-divider {
				width: 100%;
				height: 3rem;
				background-color: rgba(0, 0, 0, 0.1);
				border: solid rgba(0, 0, 0, 0.15);
				border-width: 1px 0;
				box-shadow: inset 0 0.5em 1.5em rgba(0, 0, 0, 0.1), inset 0 0.125em 0.5em rgba(0, 0, 0, 0.15);
			}

			.b-example-vr {
				flex-shrink: 0;
				width: 1.5rem;
				height: 100vh;
			}

			.bi {
				vertical-align: -0.125em;
				fill: currentColor;
			}

			.nav-scroller {
				position: relative;
				z-index: 2;
				height: 2.75rem;
				overflow-y: hidden;
			}

			.nav-scroller .nav {
				display: flex;
				flex-wrap: nowrap;
				padding-bottom: 1rem;
				margin-top: -1px;
				overflow-x: auto;
				text-align: center;
				white-space: nowrap;
				-webkit-overflow-scrolling: touch;
			}

			.btn-bd-primary {
				--bd-violet-bg: #712cf9;
				--bd-violet-rgb: 112.520718, 44.062154, 249.437846;

				--bs-btn-font-weight: 600;
				--bs-btn-color: var(--bs-white);
				--bs-btn-bg: var(--bd-violet-bg);
				--bs-btn-border-color: var(--bd-violet-bg);
				--bs-btn-hover-color: var(--bs-white);
				--bs-btn-hover-bg: #6528e0;
				--bs-btn-hover-border-color: #6528e0;
				--bs-btn-focus-shadow-rgb: var(--bd-violet-rgb);
				--bs-btn-active-color: var(--bs-btn-hover-color);
				--bs-btn-active-bg: #5a23c8;
				--bs-btn-active-border-color: #5a23c8;
			}
			.bd-mode-toggle {
				z-index: 1500;
			}

			.me-2 {
				margin-right: 0.5rem !important;
				height: 50px;
				margin-top: 0.5rem;
			}

			.product-card {
				display: flex;
				gap: 10px;
			}

			section {
				width: 400px;
			}

			section > img {
				width: 100%;
				object-fit: cover;
			}

			.navbar {
				align-items: flex-start;
			}

			.navbar-brand {
				margin-bottom: 3em;
			}

			.navbar > .container-fluid {
				flex-direction: column;
			}

			.fixed-top {
				height: 100vh;
				width: 350px;
			}

			.navbar-nav {
				flex-direction: column !important;
				justify-content: flex-start;
				gap: 4em;
			}

			nav {
				background-color: #595959;
			}

			.navbar-brand {
				font-size: 2.5em;
				color: #ebebf1;
			}

			.nav-link {
				font-size: 2em;
			}

			.flex-shrink-0 {
				display: flex !important;
				justify-content: center !important;
			}

			.container {
				margin-right: 20px !important;
				margin-left: 100px !important;
			}

			#connexion,
			#update {
				display: flex !important;
				justify-content: center !important;
				align-items: center;
				height: 70vh;
			}

			#container-connexion, {
				margin: 0 auto;
				padding-top: 2em;
				padding-left: 2em;
			}
			#connexion_btn {
				margin: 2rem 0 auto;
				text-align: center;
			}

			.col > h1 {
				margin-bottom: 1em;
			}

			.button_gap {
				width: 7%;
			}
			.button_gap_left {
				text-align: center;
			}

			#title-center {
				text-align: center;
			}

			#actions-center {
				text-align: right;
			}

			.leave-button {
				display: flex;
				justify-content: flex-end;
				margin: -20px auto;
			}

			#leave-button {
				background-color: #bcbcbc;
				font-size: 1.5em;
			}

			.produit-size {
				font-size: 1.2em;
			}

			.row_connexion {
				margin-top: 2em !important;
				margin-left: 2em !important;
			}


			/* #message{
				margin-top: 0;
			} */
		</style>


		<!-- Custom styles for this template -->
		<link href="sticky-footer-navbar.css" rel="stylesheet">
	</head>


	<header>
		<!-- Fixed navbar -->

		{% if session.login %}
			<div class="leave-button">
				<a href="/logout">
					<button id="leave-button" type="button" class="btn btn-outline-secondary ">
						<i class="fa-solid fa-right-to-bracket"></i>
					</button>
				</a>
			</div>
		{% endif %}

		<nav class="navbar navbar-expand-md navbar-dark fixed-top">
			<div class="container-fluid">
				<a class="navbar-brand" href="/">Online Snackies</a>
				<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div class="collapse navbar-collapse" id="navbarCollapse">
					<ul class="navbar-nav me-auto mb-2 mb-md-0">
						{% if not session.login.Is_Admin %}
							<li class="nav-item">
								<a class="nav-link" href="/">Connexion</a>
							</li>

						{% else %}
							<li class="nav-item">
								<a class="nav-link" href='/employees'>Employes</a>
							</li>
							<li class="nav-item">
								<a class="nav-link active" aria-current="page" href='/products'>Produits</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href='/orders'>Orders</a>
							</li>
						{% endif %}
					</ul>
					{# session : {{session.login['Is_Admin']}} #}
					{# <form class="d-flex " role="search">
																																																				<input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
																																																				<button class="btn btn-outline-success" type="submit">Search</button>
																																																			</form> #}

				</div>
			</div>
		</nav>

	</header>

	<!-- Begin page content -->

	<main class="flex-shrink-0">
		<div class="container"> {% block content %}{% endblock %}
			</div>
		</main>

		{# <footer class="footer mt-auto py-3 bg-body-tertiary">
																<div class="container">
																	<span class="text-body-secondary">Place sticky footer content here.</span>
																</div>
															</footer> #}


		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
	</html>
</body></html>
